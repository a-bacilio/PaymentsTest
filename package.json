    "@commitlint/config-conventional": "^15.0.0",
    "@types/bcrypt": "5.0.0",
    "@types/jest": "^27.4.0",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/morgan": "1.9.3",
    "babel-jest": "^27.4.2",
    "commitlint": "^15.0.0",
    "eslint": "^7.9.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.0",
    "jest": "^27.4.3",
